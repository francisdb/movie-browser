<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE jnlp PUBLIC "-//Sun Microsystems, Inc.//DTD JNLP 1.5//EN" "http://www.netbeans.org/jnlp/DTD/jnlp.dtd">
<jnlp spec="1.0+" codebase="http://moviebrowser.somatik.be/" href="$outputFile">
    <information>
        <title>Movie browser</title>
        <vendor>Somatik</vendor>
        <homepage href="http://moviebrowser.somatik.be"/>
        <description>Movie browser</description>
        <icon href="images/movie.gif"/>
        <offline-allowed />
        <shortcut>
            <desktop />
            <menu submenu="moviebrowser" />
        </shortcut>
    </information>
    <resources locale="" os="">
        <j2se href="http://java.sun.com/products/autodl/j2se" version="1.6+" initial-heap-size="32m" max-heap-size="128m" />
        $dependencies
    </resources>
    <security>
        <all-permissions/>
    </security>
    
    <application-desc main-class="$mainClass" />
    
    
</jnlp>
